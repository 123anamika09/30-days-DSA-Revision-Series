class Solution {
    public int[] shuffle(int[] nums, int n) {
        int m = nums.length;
        int[] ans = new int[nums.length];
        int idx = 0;
        for(int i = 0;i<n;i++){ // 2 part me divide 
            ans[idx++] = nums[i]; // x part me divide 
            ans[idx++] = nums[i+n]; // y divide
  
         }
        return ans;
    }
}
